stock BuyableFurniture_GetCategory(furnitureid)
{
	if(furnitureid < 0 || furnitureid >= sizeof(BuyableFurnitures))
		return -1;
	return BuyableFurnitures[furnitureid][E_FURNITURE_CATEGORY];
}

stock BuyableFurniture_GetModel(furnitureid)
{
	if(furnitureid < 0 || furnitureid >= sizeof(BuyableFurnitures))
		return -1;
	return BuyableFurnitures[furnitureid][E_FURNITURE_MODEL];
}

stock BuyableFurniture_GetName(furnitureid, name[])
{
	if(furnitureid < 0 || furnitureid >= sizeof(BuyableFurnitures))
		return 0;
	set(name, BuyableFurnitures[furnitureid][E_FURNITURE_NAME]);
	return 1;
}

stock BuyableFurniture_GetPrice(furnitureid)
{
	if(furnitureid < 0 || furnitureid >= sizeof(BuyableFurnitures))
		return 0;
	return BuyableFurnitures[furnitureid][E_FURNITURE_PRICE];
}